{
  "name": "index-alignment",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codefresh-io/index-alignment.git"
  },
  "packageManager": "yarn@4.5.2",
  "scripts": {
    "build": "rimraf ./dist && tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "dump:classic": "node ./dist/index.js dump -u $MONGO_V1_URI --tls",
    "dump:gitops": "node ./dist/index.js dump -u $MONGO_V2_URI --tls"
  },
  "engines": {
    "node": "^22"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@types/node": "^22.12.0",
    "@vitest/coverage-v8": "^3.1.2",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "mongodb": "^6.16.0"
  }
}
